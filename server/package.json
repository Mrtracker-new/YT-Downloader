    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.8",
    "bull": "^4.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "sanitize-filename": "^1.6.3",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
